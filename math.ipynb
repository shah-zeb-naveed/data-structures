    "# log of a value vs base used\n",
    "\n",
    "import math\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "hs = []\n",
    "x = 1000\n",
    "for i in range(10, 100, 1):\n",
    "    h = math.log(x, i)\n",
    "    # print(h)\n",
    "\n",
    "    hs.append(h)\n",
    "\n",
    "plt.plot(hs)"
   ]
  }
