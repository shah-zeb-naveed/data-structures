   "source": [
    "# dynamic programming and memoization\n",
    "# fibonacci example\n",
    "\n",
    "def fibo(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "\n",
    "    if not n in memo:\n",
    "        memo[n] = fibo(n - 1) + fibo(n - 2)\n",
    "\n",
    "    return memo[n]\n",
    "\n",
    "# caching\n",
    "memo = {}\n",
    "fibo(6)"
   ]
  }
